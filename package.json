{
  "name": "lookie",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16.15.0"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run start",
    "start": "npm run delete && tsc && cp -R ./src/keys/ dist/keys && node dist/index.js",
    "delete": "rm -rf dist logs",
    "dev": " npm run logClean && tsc && node dist/index.js",
    "postinstall": "tsc && mv -i -v public dist/",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "serve": "node -r dotenv/config build/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "clean": "rm -rf ./dist",
    "logClean": "rm -rf logs",
    "build-ts": "tsc",
    "eslint": "eslint . --ext .js,.ts",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "jest --forceExit --detectOpenHandles --coverage --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "mongoose": "^6.4.2",
    "nodemailer": "^6.7.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jwt-simple": "^0.5.33",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.9",
    "@types/passport-jwt": "^3.0.6"
  }
}